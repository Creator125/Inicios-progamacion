//Operadores de asignación (Relacionales)

Proceso pricipal
	Definir resultado Como Logico;
	Definir a, b, c Como Entero;
	
	Escribir "resultado de a";
	leer a;
	Escribir "resultado de b";
	leer b;
	Escribir "resultado de c";
	leer c;
	
	resultado  <- a+b > c;
	
	Escribir "El resultado es: ", resultado;
FinProceso
