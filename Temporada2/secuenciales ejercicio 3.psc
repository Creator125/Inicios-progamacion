Proceso ejercicio3
	Definir num1, num2, suma, resta, multi, divi Como Real;
	
	Escribir "Escriba un numero";
	Leer num1;
	Escribir "Escriba otro numero";
	Leer num2;
	
	//Suma
	suma = num1 + num2;
	
	//Resta
	resta = num1 - num2;
	
	//Multiplicación
	multi = num1 * num2;
	
	//Division
	divi = num1 / num2;
	
	Imprimir "Tu resultado son: ";
	Imprimir "Suma: ",suma;
	Imprimir "Resta: ",resta;
	Imprimir "Multiplicación: ",multi;
	Imprimir "División: ",divi;
	
FinProceso
